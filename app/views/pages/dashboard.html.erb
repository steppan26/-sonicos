<div class="container" data-controller="dashboard">
  <h1>Pages#home</h1>
  <p>Welcome home!!</p>
  <div class="dashboard-tabs">
    <div class="tab active" data-action="click->dashboard#toggle" data-page-name="homepage">Home</div>
    <div class="tab" data-action="click->dashboard#toggle" data-page-name="search">Search a Sonic</div>
    <div class="tab" data-action="click->dashboard#toggle" data-page-name="new">New Sonic</div>
  </div>
  <div class="dashboard-wrapper container" data-dashboard-target="dashboard-wrapper">
      <%= render 'homepage' %>
      <%= render 'search-costume' %>
      <%= render 'new-costume', costume: @costume %>
  </div>
</div>
