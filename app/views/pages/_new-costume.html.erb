<div id="new" class="mt-3 hidden pb-5" data-dashboard-target="section">
  <h3 class="my-5">Create a new Sonic</h3>

<%= simple_form_for costume do |f| %>
  <%= f.input :title %>
  <%= f.input :price %>
  <%= f.input :description %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.input :location %>
  <%= f.submit "Go Faster" %>
<% end %>
</div>
